{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {
    },
    "rules": {
      "max-line-length": {
        "options": [80]
      },
      "object-literal-sort-keys": false,
      "no-consecutive-blank-lines": false,
      "no-empty": false,
      "comment-format": [ true, "check-space" ],
      "curly": false
    },
    "rulesDirectory": []
}
